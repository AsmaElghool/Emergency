<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/emergancyCallFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.EmergencyCall">

    <ImageView
        android:id="@+id/imgEmergancyCall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_16sdp"
        android:layout_marginTop="@dimen/_70sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:src="@drawable/emergancy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="188dp"
        android:layout_height="31dp"
        android:layout_marginStart="@dimen/_70sdp"
        android:layout_marginTop="@dimen/_50sdp"
        android:src="@drawable/ic_redcircle"
        app:layout_constraintBottom_toTopOf="@+id/imgEmergancyCall"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:layout_width="19dp"
        android:layout_height="58dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="36dp"
        android:src="@drawable/ic_greencircle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="19dp"
        android:layout_height="58dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="264dp"
        android:src="@drawable/ic_brown"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>