<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splash">

    <fragment
        android:id="@+id/splash"
        android:name="myproject1.ash1.emergency.ui.Splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splash_to_emergencyCall"
            app:destination="@id/emergencyCall" />
        <action
            android:id="@+id/action_splash_to_socialMedia"
            app:destination="@id/socialMedia" />
    </fragment>
    <fragment
        android:id="@+id/emergencyCall"
        android:name="myproject1.ash1.emergency.ui.EmergencyCall"
        android:label="fragment_emergency_call"
        tools:layout="@layout/fragment_emergency_call" >
        <action
            android:id="@+id/action_emergencyCall_to_firstAidKit"
            app:destination="@id/firstAidKit" />
        <action
            android:id="@+id/action_emergencyCall_to_socialMedia"
            app:destination="@id/socialMedia" />
    </fragment>
    <fragment
        android:id="@+id/firstAidKit"
        android:name="myproject1.ash1.emergency.ui.FirstAidKit"
        android:label="fragment_first_aid_kit"
        tools:layout="@layout/fragment_first_aid_kit" >
        <action
            android:id="@+id/action_firstAidKit_to_socialMedia"
            app:destination="@id/socialMedia" />
        <action
            android:id="@+id/action_firstAidKit_to_socialMedia2"
            app:destination="@id/socialMedia" />
    </fragment>
    <fragment
        android:id="@+id/socialMedia"
        android:name="myproject1.ash1.emergency.ui.SocialMedia"
        android:label="fragment_social_media"
        tools:layout="@layout/fragment_social_media" >
        <action
            android:id="@+id/action_socialMedia_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="myproject1.ash1.emergency.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>