<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splash">

    <fragment
        android:id="@+id/splash"
        android:name="myproject1.ash1.emergency.ui.Splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splash_to_onboarding"
            app:destination="@id/onboarding" />
        <action
            android:id="@+id/action_splash_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="myproject1.ash1.emergency.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/onboarding"
        android:name="myproject1.ash1.emergency.Onboarding"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" />

</navigation>